game.StarterGui:SetCore("SendNotification", {
	Title = "PhenomWare By Clea And Vet";
	Text = "Script Successfully Loaded";
	Button = "Thanks Clea";
	Duration = 5;
})
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
    local Window = Library.CreateLib("PhenomWare", "GrapeTheme")
 
    -- MAIN
    local Main = Window:NewTab("Main")
    local MainSection = Main:NewSection("Main")

    MainSection:NewButton("Loom's Aimbot", "Gives you Aimbot", function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Loompic/Phenom-Aimbot/main/Bot"))()
    end)
    
     MainSection:NewButton("Loom's Aimbot", "Gives you Aimbot", function()
        
    
     MainSection:NewButton("TBot (X)", "Just press x when  green", function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/carolesdaughter/Roblox-Phenom-Silent-Aims/main/TBot.lua"))()
    end)
    _G.brah = false
_G.keybind = "Z"
game.Players.LocalPlayer:GetMouse().KeyDown:connect(
function(key)
if key:lower() == _G.keybind and _G.brah == false then
_G.brah = true
for i, v in pairs(game.Players:GetPlayers()) do
if v.Name ~= game.Players.LocalPlayer.Character.Name and (v.Character.Torso.CFrame.p - game.Players.LocalPlayer.Character.Torso.CFrame.p).Magnitude < 20 and v.Character.Basketball then
while _G.brah == true do
if v.Character.Torso.Velocity.magnitude > 0.2 then
game.Players.LocalPlayer.Character.Humanoid:MoveTo(v.Character.Torso.CFrame.p + v.Character.Torso.Velocity.unit * 7)
else
game.Players.LocalPlayer.Character.Humanoid:MoveTo(v.Character.Torso.CFrame.p)
end
wait()
end
end
end
elseif key:lower() == _G.keybind and _G.brah == true then
_G.brah = false
end
end)
     MainSection:NewButton("NormalSIlent (X)", "Just press x when  green", function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/carolesdaughter/Roblox-Phenom-Silent-Aims/main/NormalAimbot.lua"))()
    end)
    
    MainSection:NewButton("BounceBot (X)", "Just press x when  green", function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/carolesdaughter/Roblox-Phenom-Silent-Aims/main/BounceBot.lua"))()
    end)
    
    MainSection:NewButton("Ball Mag (Press R)", "Gives you Ball Mag", function()
        local UIS = game:GetService('UserInputService')
        local plr = game.Players.LocalPlayer
        local Char = plr.Character or plr.CharacterAded:Wait()
        local Key = 'R'
        UIS.InputBegan:Connect(function(Input, IsTyping)
        if IsTyping then return end
        local KeyPressed = Input.KeyCode
        if KeyPressed == Enum.KeyCode[Key] then
        game.Workspace.Map.Basketball.Handle.CFrame = Game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
        end
        end)
    end)
    MainSection:NewToggle("Jump All (Z)", "Get ball first", function()
    local UIS = game:GetService("UserInputService")
local bounceAll = false


UIS.InputBegan:Connect(function(input, gameProcessedEvent)
    if gameProcessedEvent then return end
            local char = game.Players.LocalPlayer.Character
           if input.KeyCode == Enum.KeyCode.Z then
                local x = 0

while x < 100 do
    print (x)
    x = x + 1
    wait(0.1)
          for i  , v in pairs(workspace:GetDescendants()) do 
                if v.Name == "Left Leg" then
                   firetouchinterest(char.Head,v, 0)
                   wait()
                   firetouchinterest(char.Head,v, 1) 
        end
      end
     end
    end 
end)

    
     MainSection:NewToggle("Auto Ball Mag", "Auto Mags Ball", function()
getgenv().Automag = Value;

while wait() do
if getgenv().Automag == true then
for i, v in pairs(Workspace:GetDescendants()) do
    if v.Name == "rim" then
        firetouchinterest(Torso, v.Parent,0)
        wait()
        firetouchinterest(Torso, v.Parent,1)

    end
end
end

end
    end)


    -- PLAYER
    local Player = Window:NewTab("Player")
    local PlayerSection = Player:NewSection("Player")
    
    

    PlayerSection:NewButton("Infinite Jumps", "Gives you Infinite Jumps", function()    
    local InfiniteJumpEnabled = true
    game:GetService("UserInputService").JumpRequest:connect(function()
	if InfiniteJumpEnabled then
		game:GetService"Players".LocalPlayer.Character:FindFirstChildOfClass'Humanoid':ChangeState("Jumping")
	end
    end)

    end)
    
PlayerSection:NewButton("Trail/Basketball For Someting", "Prob Something", function()    
local Player = game:GetService("Players").LocalPlayer
local Character = Player.Character

Character.ChildAdded:Connect(function(Child)
    if Child.Name == ("Basketball") then
          Character:FindFirstChild("Basketball").Ball.SecondaryTrail.Enabled = true
          Character:FindFirstChild("Basketball").Ball.TertiaryTrail.Enabled = true
          
    end
end)


    -- Teleportation
    local Teleportation = Window:NewTab("Teleportation")
    local TPSection = Teleportation:NewSection("Teleportation")

    TPSection:NewButton("RimTp1", "Tps you to a certain rim", function()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-16.9935493, 42.9078331, 143.324341, -1, 0, 0, 0, 1, 0, 0, 0, -1) 
    end)

    TPSection:NewButton("RimTp2", "Tps you to a certain rim", function()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-16.9944324, 42.6078377, -46.1025467, 1, 0, 0, 0, 1, 0, 0, 0, 1)
    end)

    TPSection:NewButton("BallSpawn1", "Tps you to a Ball Spawner", function()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-16.9139996, 27.8899994, -32.2940369, -1.1920929e-07, -0, -1.00000012, 0, 1, -0, 1.00000012, 0, -1.1920929e-07)
    end)

    TPSection:NewButton("BallSpawn2", "Tps you to a Ball Spawner", function()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-17.0290585, 27.8799992, 128.626984, -1.1920929e-07, 0, 1.00000012, 0, 1, 0, -1.00000012, 0, -1.1920929e-07)
    end)
    
    

    -- Credits

    local Credits = Window:NewTab("Credits")
    local CreditsSection = Credits:NewSection("Credits")

    CreditsSection:NewLabel("Gui Maker:Clea And Vet")
    CreditsSection:NewLabel("Scripting Assistant: Clea")
    CreditsSection:NewLabel("Scripter:Vet")
    CreditsSection:NewLabel("Aimbot Maker: Loompic")
    CreditsSection:NewLabel("Silent Aim Maker: m_gnet")



    --Settings
    local Settings = Window:NewTab("Settings")
    local SettingsSection = Settings:NewSection("Settings")

    SettingsSection:NewButton("Reset", "Resets your Characters", function()
        game.Players.LocalPlayer.Character.Humanoid.Health = 0
    end)

    SettingsSection:NewButton("Rejoins", "Rejoins your game", function()
        local TeleportService = game:GetService("TeleportService")
        local placeID = 623694595
        local player = Game.Players.LocalPlayer
         
        TeleportService:Teleport(placeID, player)
    end)

    SettingsSection:NewKeybind("Toggle", "Toggles the Gui", Enum.KeyCode.F, function()
        Library:ToggleUI()
    end)
end)
end)
