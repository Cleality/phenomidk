game.StarterGui:SetCore("SendNotification", {
	Title = "Phenom";
	Text = "Script Successfully Loaded";
	Duration = 5;
})
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
    local Window = Library.CreateLib("Phenom", "GrapeTheme")
    --MAIN
local Main = Window:NewTab("Scripties")
local MainSection = Main:NewSection("Scripties")
 
MainSection:NewButton("TBot", "Phenom", function()
   loadstring(game:HttpGet('https://github.com/carolesdaughter/Roblox-Phenom-Silent-Aims/blob/main/TBot.lua'))()
end)

MainSection:NewButton("Silent aiM!!!!!", " All Power Silent Aim!", function()
    loadstring(game:HttpGet('https://pastebin.com/raw/yPY7v700',true))();
end)
 
MainSection:NewButton("Bounce Silent aim", "x", function()
    loadstring(game:HttpGet('https://pastebin.com/raw/SY8f8Srg'))()
end)
 
MainSection:NewButton("fc dunk", "look at rim press x", function()
    loadstring(game:HttpGet('https://pastebin.com/raw/FU2P2ujK'))()
end)
 
MainSection:NewButton("aimstudio checker", "click to see", function()
    loadstring(game:HttpGet('https://pastebin.com/raw/vBPYpCaj'))()
end)

MainSection:NewButton("Flat Arc!", "only works at 75 line!", function()
    loadstring(game:HttpGet('https://pastebin.com/raw/0GSjPHBR',true))();
end)

MainSection:NewButton("Auto Score!", "Pushin N!", function()
    loadstring(game:HttpGet('https://pastebin.com/raw/5EaszTNq',true))();
end)

MainSection:NewButton("Follow Ball!", "Press M!", function()
    loadstring(game:HttpGet('https://pastebin.com/raw/18xusads',true))();
end)

MainSection:NewButton("Bounce All!", "Pushin P!", function()
    loadstring(game:HttpGet('https://pastebin.com/raw/kyzZb7xk',true))();
end)

MainSection:NewButton("Ball Mag (Press R)", "Gives you Ball Mag", function()
        local UIS = game:GetService('UserInputService')
        local plr = game.Players.LocalPlayer
        local Char = plr.Character or plr.CharacterAded:Wait()
        local Key = 'R'
        UIS.InputBegan:Connect(function(Input, IsTyping)
        if IsTyping then return end
        local KeyPressed = Input.KeyCode
        if KeyPressed == Enum.KeyCode[Key] then
        game.Workspace.Map.Basketball.Handle.CFrame = Game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
        end
        end)
    end)

-- PLAYER
    local Player = Window:NewTab("Player")
    local PlayerSection = Player:NewSection("Player")
 
 
 
    PlayerSection:NewButton("Infinite Jumps", "Gives you Infinite Jumps", function()    
    local InfiniteJumpEnabled = true
    game:GetService("UserInputService").JumpRequest:connect(function()
	if InfiniteJumpEnabled then
		game:GetService"Players".LocalPlayer.Character:FindFirstChildOfClass'Humanoid':ChangeState("Jumping")
	end
    end)
 
    end)
 
PlayerSection:NewButton("admin", "you have admin", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/fatesc/fates-admin/main/main.lua"))();
end)
 
 
--Settings
    local Settings = Window:NewTab("Settings")
    local SettingsSection = Settings:NewSection("Settings")
 
    SettingsSection:NewButton("Reset", "Resets your Characters", function()
        game.Players.LocalPlayer.Character.Humanoid.Health = 0
    end)
 
    SettingsSection:NewButton("Rejoins", "Rejoins your game", function()
        local TeleportService = game:GetService("TeleportService")
        local placeID = 623694595
        local player = Game.Players.LocalPlayer
 
        TeleportService:Teleport(placeID, player)
    end)
 
    SettingsSection:NewKeybind("Toggle", "Toggles the Gui", Enum.KeyCode.LeftControl, function()
        Library:ToggleUI()
    end)
